{
    "name": "rustbot",
    "version": "1.0.0",
    "private": true,
    "description": "A lightweight Rust server management bot",
    "scripts": {
      "install": "cd backend && npm install && cd ../frontend && npm install",
      "build": "cd frontend && npm run build",
      "start": "cd backend && npm start",
      "dev": "concurrently \"cd backend && npm run dev\" \"cd frontend && npm run dev\"",
      "start:prod": "cd backend && npm start"
    },
    "keywords": ["rust", "bot", "management"],
    "author": "Your Name",
    "license": "MIT",
    "engines": {
      "node": ">=16.0.0"
    },
    "dependencies": {
      "nodemon": "^3.0.3",
      "vite": "^5.1.4"
    },
    "devDependencies": {
      "concurrently": "^8.2.2"
    },
    "overrides": {
      "uuid": "^9.0.1",
      "request": "^2.88.2",
      "request-promise": "^4.2.6",
      "har-validator": "^5.1.5"
    }
}
  