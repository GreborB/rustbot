# CORS Configuration
CORS_ORIGIN=http://localhost:3000

# Logging Configuration
LOG_LEVEL=debug

# WebSocket Configuration
WS_PATH=/ws
WS_PING_INTERVAL=30000
WS_PING_TIMEOUT=5000 